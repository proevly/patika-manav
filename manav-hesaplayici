import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);

        System.out.print("Elma Kaç Kilo :  ");
        int elma = input.nextInt();
        System.out.print("Armut Kaç Kilo :");
        int armut = input.nextInt();
        System.out.print("Domates Kaç Kilo :  ");
        int domates = input.nextInt();
        System.out.print("Muz Kaç Kilo :");
        int muz = input.nextInt();
        System.out.println("Patlıcan Kaç Kilo :");
        int patlican = input.nextInt();

        double sonuc = (elma*3.67)+(armut*2.14)+(domates*1.11)+(muz*0.95)+(patlican*5);
        System.out.println("Toplam Fiyat: "+sonuc);
        
    }
}
